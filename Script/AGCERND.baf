	GlobalTimerExpired("CerndReactDerilQuestDone01","GLOBAL")
	Global("ToggleCerndCommentDerilResolution", "GLOBAL", 1)
	Global("CerndReactDerilQuestDone", "GLOBAL", 0)
THEN
