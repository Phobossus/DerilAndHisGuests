IF
	//InParty(Myself)
	Global("DerilQuestDone", "GLOBAL", 1)
	Global("ToggleCerndCommentDerilResolution", "GLOBAL", 0)
THEN
	RESPONSE #100
		SetGlobal("ToggleCerndCommentDerilResolution", "GLOBAL", 1)
END

